
record(bo,"$(Name)-SetEncSourceType"){
	field(DTYP,"stream")
	field(OUT,"@expression.proto setEncSourceType($(Index)) $(PORT)")
	field(ZNAM,"FALSE")
	field(ONAM,"TRUE")
}

record(bi,"$(Name)-GetEncSourceType"){
	field(DTYP,"stream")
	field(INP,"@expression.proto getEncSourceType($(Index)) $(PORT)")
	field(ZNAM,"FALSE")
	field(ONAM,"TRUE")
        field(SCAN, "1 second")
}


