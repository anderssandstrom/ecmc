
record(bo,"$(P)Storage$(Index)-Clear"){
	field(DTYP,"stream")
	field(OUT,"@dataStorage.proto clearDataStorage($(Index)) $(PORT)")
	field(ZNAM,"FALSE")
	field(ONAM,"TRUE")
	field(FLNK,"$(P)Storage$(Index)-Data")
}

record(waveform,"$(P)Storage$(Index)-Data"){
	field(DTYP,"stream")
	field(INP,"@dataStorage.proto getDataStorageData($(Index)) $(PORT)")
	field(NELM,1000)
        field(FTVL,"DOUBLE")
}

record(ai,"$(P)Storage$(Index)-DataIndex"){
	field(DTYP,"stream")
	field(INP,"@dataStorage.proto getDataIndex($(Index)) $(PORT)")
	field(SCAN,"2 second")
	field(FLNK,"$(P)Storage$(Index)-Data")
}
