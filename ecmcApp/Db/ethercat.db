
record(ao,"SetEtherCatSlave$(S)Entry$(E)"){
	field(DTYP,"stream")
	field(OUT,"@ethercat.proto setPdoEntry($(S),$(E)) $(PORT)")
	field(FLNK,"GetEtherCatSlave$(S)Entry$(E)")
}

record(ai,"GetEtherCatSlave$(S)Entry$(E)"){
	field(DTYP,"stream")
	field(INP,"@ethercat.proto getPdoEntry($(S),$(E)) $(PORT)")
	field(SCAN,"1 second")
}

